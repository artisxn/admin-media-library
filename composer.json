    "require": {
        "php": "^7.4.0",
        "laravel/framework": "^7.0",
        "laravel/nova": "^3.0",
        "codicastudio/media-library": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "codicastudio\\AdvancedNovaMediaLibrary\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "codicastudio\\AdvancedNovaMediaLibrary\\AdvancedNovaMediaLibraryServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
